{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",

  "rushVersion": "5.24.4",
  "yarnVersion": "1.22.4",
  "nodeSupportedVersionRange": ">=12.0.0 <13.0.0",
  "projectFolderMaxDepth": 2,
  "repository": {
    "url": "https://github.com/routerating/route-rating-web",
    "defaultBranch": "master",
    "defaultRemote": "origin"
  },
  "projects": [
    {
      "packageName": "@routerating/requests",
      "projectFolder": "sdks/requests",
      "shouldPublish": true,
      "reviewCategory": "sdks"
    },
    {
      "packageName": "@routerating/entities",
      "projectFolder": "libraries/entities",
      "shouldPublish": true,
      "reviewCategory": "libraries"
    },
    {
      "packageName": "@routerating/web",
      "projectFolder": "apps/web",
      "shouldPublish": false,
      "reviewCategory": "apps"
    }
  ]
}
