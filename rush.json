{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",

  "rushVersion": "5.23.5",
  "npmVersion": "6.14.5",
  "nodeSupportedVersionRange": ">=11.0.0 <13.0.0",
  "projectFolderMaxDepth": 2,
  "repository": {
    "url": "https://github.com/LukeShay/route-rating-web",
    "defaultBranch": "master",
    "defaultRemote": "origin"
  },

  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },

  "projects": [
    {
      "packageName": "@routerating/api",
      "projectFolder": "sdks/api",
      "shouldPublish": true,
      "reviewCategory": "sdks"
    },
    {
      "packageName": "@routerating/entities",
      "projectFolder": "libraries/entities",
      "shouldPublish": true,
      "reviewCategory": "libraries"
    }
  ]
}
