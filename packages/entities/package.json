{
  "name": "@routerating/entities",
  "version": "0.0.2",
  "description": "Entities package for Route Rating.",
  "repository": "https://github.com/LukeShay/route-rating-web.git",
  "author": "Luke Shay <https://github.com/LukeShay>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "rollup -c",
    "format": "yarn lint --fix",
    "lint": "eslint src/ --ext .ts --ignore-path ../../.gitignore . ",
    "test": "exit 0",
    "clean": "find . -name *.log -delete && rm -rf dist"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "input": "src/index.ts",
  "main": "dist/cjs/index.js",
  "esm": "dist/es/index.js",
  "cjs": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/es/index.js"
  },
  "devDependencies": {
    "@lukeshay/eslint-config-typescript": "^1.0.2",
    "@lukeshay/typescript-config": "^1.0.0",
    "@rollup/plugin-auto-install": "^2.1.0",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@rollup/plugin-strip": "^1.3.3",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "rollup": "^2.10.9",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "typescript": "^3.9.2",
    "@lukeshay/rollup-config": "~0.0.3"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "typescript",
    "route-rating"
  ]
}
