{
  "name": "@routerating/requests",
  "private": true,
  "version": "0.0.5",
  "description": "Requests package for Route Rating.",
  "repository": "https://github.com/LukeShay/route-rating-web.git",
  "author": "Luke Shay <https://github.com/LukeShay>",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn clean",
    "build": "yarn build:js && yarn build:types",
    "build:js": "rollup -c",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly --outDir dist/types",
    "format": "yarn lint --fix",
    "lint": "eslint src/ --ext .ts --ignore-path ../../.gitignore . ",
    "test": "jest",
    "test:cov": "jest --coverage",
    "watch": "rollup -cw",
    "clean": "find . -name *.log -delete && rm -rf dist coverage junit.xml"
  },
  "input": "src/index.ts",
  "main": "dist/cjs/index.js",
  "esm": "dist/es/index.js",
  "cjs": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/es/index.js"
  },
  "dependencies": {
    "@routerating/entities": "^0.0.3",
    "tslib": "^2.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "typescript",
    "route-rating"
  ]
}
